<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.photoapp53">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".NewAlbumScreen"></activity>

        <activity android:name=".RenameScreen"></activity>

        <activity android:name=".PhotoScreen"></activity>

        <activity
            android:name=".AlbumScreen"
            android:parentActivityName=".MainActivity"
            android:grantUriPermissions = "true"
            android:permission="android.permission.MANAGE_DOCUMENTS">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name=".MovePhotoScreen"></activity>

        <activity android:name=".EditTags"></activity>

        <activity android:name=".Search"></activity>
        <activity android:name=".Results"></activity>
    </application>

</manifest>